  "name": "cleo",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "gen:key": "node ./scripts/generate-encryption-key.js",
    "docker:build": "./scripts/docker-build.sh",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down --volumes --remove-orphans",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.override.yml up -d --build && docker-compose logs -f cleo",
    "docker:dev-bg": "docker-compose -f docker-compose.yml -f docker-compose.override.yml up -d --build",
    "docker:dev-down": "docker-compose -f docker-compose.yml -f docker-compose.override.yml down --volumes --remove-orphans && docker volume prune -f",
    "docker:dev-restart": "docker-compose -f docker-compose.yml -f docker-compose.override.yml restart cleo && docker-compose logs -f cleo",
    "docker:prod": "docker-compose -f docker-compose.yml up --build",
    "docker:logs": "docker-compose logs -f cleo",
    "docker:logs-all": "docker-compose logs -f",
    "docker:clean": "docker-compose down --volumes --remove-orphans && docker system prune -f && docker volume prune -f",
    "docker:clean-all": "docker-compose down --volumes --remove-orphans && docker system prune -af --volumes && docker builder prune -af",
    "docker:reset": "docker-compose down --volumes --remove-orphans && docker system prune -af --volumes && docker builder prune -af",
    "docker:status": "docker-compose ps && echo '\n--- Docker System Usage ---' && docker system df",
    "docker:shell": "docker-compose exec cleo /bin/bash",
    "dev:helper": "node scripts/dev-helper.js"
  },
