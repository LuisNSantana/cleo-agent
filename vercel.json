{
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 180
    },
    "app/api/*/*/route.ts": {
      "maxDuration": 180
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/|_next/|favicon.ico|robots.txt|sitemap.xml|manifest.json|assets/).*)",
      "destination": "/"
    }
  ]
}