# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key
SUPABASE_ACCESS_TOKEN=your_supabase_access_token
SUPABASE_PROJECT_REF=your_supabase_project_ref

# CSRF Protection (required)
CSRF_SECRET=your_32_character_random_string

# Logging Configuration
NODE_ENV=development
ENABLE_DEBUG_LOGS=false

# AI Model API Keys
OPENAI_API_KEY=sk-your_openai_api_key
MISTRAL_API_KEY=your_mistral_api_key
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
XAI_API_KEY=your_xai_api_key
PERPLEXITY_API_KEY=your_perplexity_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# Ollama Configuration (for local AI models)
OLLAMA_BASE_URL=http://localhost:11434

# Developer Tools (optional)
EXA_API_KEY=your_exa_api_key
GITHUB_TOKEN=your_github_token

# Production Configuration (optional)
NEXT_PUBLIC_VERCEL_URL=your_production_domain
NEXT_PUBLIC_ENABLE_TRADINGVIEW=true

# Development Tools (optional)
ANALYZE=false

# Voice Mode Configuration
WS_PROXY_PORT=8080
NEXT_PUBLIC_WS_PROXY_URL=ws://localhost:8080

# ElevenLabs (fallback) Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key
NEXT_PUBLIC_ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id

# Caching (optional, hybrid L1+L2)
# Enable Redis-backed L2 cache for RAG retrieval and future features
ENABLE_REDIS_CACHE=false

# Option A: Upstash (preferred for serverless)
# Get these from your Upstash Redis database dashboard
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Option B: Standard Redis (e.g., Redis Cloud) â€” use a rediss:// URL for TLS
REDIS_URL=
